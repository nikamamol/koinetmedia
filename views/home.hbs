<script>
   document.addEventListener('DOMContentLoaded', () => {
    // Fetch blog posts from backend API
    fetch('/api/blogs')
        .then(response => {
            if (!response.ok) {
                throw new Error('Network response was not ok');
            }
            return response.json();
        })
        .then(posts => {
            // Sort posts by date to ensure latest ones are first
            posts.sort((a, b) => new Date(b.date) - new Date(a.date));

            const blogPostsElement = document.querySelector('#blogPosts .row');
            
            // Iterate through only the first 3 posts (latest posts)
            posts.slice(0, 3).forEach(post => {
                const postElement = document.createElement('div');
                postElement.classList.add('col-lg-4', 'col-md-6', 'wow', 'fadeInUp');
                postElement.setAttribute('data-wow-delay', '0.1s');
                postElement.innerHTML = `
                    <div class="service-item position-relative h-100">
                        <div class="service-text shadow blogHover">
                            <div class="mb-4">
                                <img class="img-fluid" src="${post.imageUrl}" alt="Blog Image">
                            </div>
                            <div class="p-4">
                                <a href="/viewblog/${post.id}" class="mb-3 cursor-pointer fs-4 fw-bold text-decoration-none text-dark">${post.title}</a>
                               
                            </div>
                        </div>
                        <div class="service-btn rounded-0 rounded-bottom">
                            <a class="text-danger fw-medium" href="/viewblog/${post.id}">Read More<i class="bi bi-chevron-double-right ms-2"></i></a>
                        </div>
                    </div>
                `;
                blogPostsElement.appendChild(postElement);
            });
        })
        .catch(error => {
            console.error('Error fetching blog posts:', error);
            // Handle error display or logging as needed
        })
        .finally(() => {
            // Remove spinner when done loading
            const spinner = document.getElementById('spinner');
            if (spinner) {
                spinner.remove();
            }
        });
});

</script>

